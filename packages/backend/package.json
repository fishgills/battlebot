{
  "name": "@battlebot/server",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development nest start --debug 9230 --watch",
    "start-brk": "NODE_ENV=development nest start --exec 'node --inspect-brk=9230' --watch",
    "start:prod": "node dist/main",
    "typeorm": "typeorm-ts-node-commonjs",
    "entity:create": "yarn typeorm entity:create -n",
    "migrate:generate": "yarn typeorm migration:generate ./src/typeorm/migrations/",
    "migrate:run": "yarn typeorm migration:run",
    "migrate:revert": "yarn typeorm migration:revert",
    "schema:drop": "yarn typeorm schema:drop"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.245.0",
    "@dice-roller/rpg-dice-roller": "^5.2.1",
