{
  "display_information": {
    "name": "BattleBot (Dev)",
    "description": "Running off my laptop",
    "background_color": "#ff00ff"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": false,
      "messages_tab_enabled": true,
      "messages_tab_read_only_enabled": false
    },
    "bot_user": {
      "display_name": "DevelopmentBot",
      "always_online": true
    }
  },
  "oauth_config": {
    "redirect_urls": ["https://bot.battlebot.ngrok.io/slack/oauth_redirect"],
    "scopes": {
      "bot": ["app_mentions:read", "users:read"]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": ["team_join", "app_mention"]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
