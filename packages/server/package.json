{
  "name": "@battlebot/server",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=development DEBUG=* nest start --debug 9230 --watch",
    "start:prod": "node dist/main",
    "typeorm:cli": "node --require ts-node/register node_modules/typeorm/cli.js -f ./src/ormconfig-migrations.ts",
    "entity:create": "yarn typeorm:cli entity:create -n",
    "migrate:generate": "yarn typeorm:cli migration:generate -n",
    "migrate:run": "yarn typeorm:cli migration:run",
    "migrate:revert": "yarn typeorm:cli migration:revert",
    "schema:drop": "yarn typeorm:cli schema:drop"
  },
  "dependencies": {
    "@dice-roller/rpg-dice-roller": "^5.1.0",
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.7",
    "@nestjs/core": "^8.2.6",
    "@nestjs/graphql": "^10.0.2",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/typeorm": "^8.0.3",
    "apollo-server-core": "^3.6.1",
    "apollo-server-express": "^3.6.1",
    "axios": "^0.24.0",
    "axios-debug-log": "^0.8.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.0",
    "dataloader": "^2.0.0",
    "date-fns": "^2.28.0",
    "dd-trace": "^1.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "mysql2": "^2.3.3",
    "nest-graphql-scalar-adapter": "^1.1.2",
    "openid-client": "^5.1.2",
    "passport": "^0.5.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.1",
    "source-map-support": "^0.5.21",
    "stripe": "^8.203.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.6",
    "@types/express-session": "^1.17.4",
    "@types/openid-client": "^3.7.0",
    "@types/passport": "^1.0.7",
    "@types/source-map-support": "^0",
    "ts-node": "^10.4.0",
    "typescript": "~4.3.0"
  }
}
